    "html-webpack-plugin": "^2.24.1",
    "jest": "^21.1.0",
    "jest-cli": "^21.2.1",
    "jsdom": "^11.2.0",
    "jsdom-global": "^3.0.2",
    "nightmare": "^2.10.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "postcss-loader": "^2.0.6",
    "pre-commit": "^1.2.2",
    "prepush": "^3.1.11",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-svg-loader": "^1.1.1",
